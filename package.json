{
  "name": "estebanborai.com",
  "version": "0.2.4",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "deploy:production": "npm run build && npm run wrangler:publish:production",
    "deploy:staging": "npm run build && npm run wrangler:publish:staging",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "wrangler:publish:production": "wrangler publish --env production",
    "wrangler:publish:staging": "wrangler publish --env staging"
  },
  "type": "module",
  "devDependencies": {
    "@sentry/browser": "^7.1.1",
    "@sentry/tracing": "^7.1.1",
    "@sveltejs/adapter-cloudflare": "^1.0.0-next.30",
    "@sveltejs/kit": "^1.0.0-next.350",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/typography": "^0.5.2",
    "@types/remarkable": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "autoprefixer": "^10.4.7",
    "cssnano": "^5.1.11",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "highlight.js": "^11.5.1",
    "mdsvex": "^0.10.6",
    "phosphor-svelte": "^1.2.1",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "remarkable": "^2.0.1",
    "svelte": "^3.48.0",
    "svelte-check": "^2.7.2",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.1.2",
    "tslib": "^2.4.0",
    "typescript": "^4.7.3",
    "wrangler": "^2.0.9"
  }
}
